
  <tr>
    <td>
      <%= cohort.id %>
    </td>
    <td>
      <%= cohort.name.upcase %>
    </td>
    <td>
      <%= cohort.start_date.strftime("%d/%m/%Y") %>
    </td>
    <td>
      <%= cohort.end_date.strftime("%d/%m/%Y") %>
    </td>
    <td>
     <%# <h1> <%= cohort.user.first_name</h1> %> %>
    </td>
    <td> 
      <span><%= link_to '', edit_cohort_path(cohort), id: "edit_#{cohort.id}", class: 'glyphicon glyphicon-pencil index-icons' %></span>
    </td>
    <td>
      <span><%= link_to '', cohort_path(cohort), method: :delete, id: "#{cohort.id}", class: 'glyphicon glyphicon-trash index-icons' %></span>
    </td>
    <td>
      <span><%= link_to '', new_cohort_path(cohort), id: "#{cohort.id}", class: 'glyphicon glyphicon-plus index-icons' %></span>
    </td> 
    <td>
      <span><%= link_to '', cohort_path(cohort), class: 'glyphicon glyphicon-folder-open index-icons' %></span>
    </td>  
  </tr>
      