<h2>show page</h2>
<%= @cohort.users.inspect %>

<div class="container-fluid">
  <div class="row media">
    <div class="col-md-2">
    <% @cohort.users.each do |user| %>
      <div class="media-left media-top">
        <a href="#">
          <%= image_tag(user.email, size: "66", class: "photo") %>
        </a>
      </div>
    </div>
    <div class="col-md-9 profile-info">
      <p><span>Name: </span><%= user.full_name %></p>
      <p><span>Age: </span><%= user.age %></p>
      <p><span>Education: </span><%= user.education %></p>
    </div>
  </div>

</div>
<% end %>
